	<!-- widgets -->
	<widgets>
		
		<!-- Start Image -->   
		<widget
			id="startImage"
			type="startImage"
			hideImageWhilePlayback="true"			
			percentWidth="100"
			percentHeight="100"
			imagePercentWidth="100"
			imagePercentHeight="100"
			/>	
		<!-- Ende Start Image -->
		
		<!-- Buffering Notifier -->
			
		<widget
			id="bufferNotifier"
			type="bufferNotifier"
			percentWidth="100"
			percentHeight="100"	
			loader="loader"		
			animation="loaderAnimation"
			/>
			
		<!-- Ende Buffering Notifier -->		

		<!-- Tooltips -->
		
		<widget
			id="tooltip"
			type="tooltip"
			borderColor="0x000000"
			borderThickness="1"
			backgroundColor="0xFFFFE1"
			font="tooltipFont"
			maxWidth="200"
			delay="500"
			/>		 
			
		<!-- Ende tooltips --> 
		
		<!-- Overlay PlayButton -->
		
		<widget
			id="overlayPlayButton"
			type="overlayPlayButton"
			upFace="overlayPlayUp"
			downFace="overlayPlayDown"
			disabledFace="overlayPlayDisabled"
			buttonMode="true"	
			left="0"
			right="0"
			top="45"
			bottom="45"
			/>
				
		<!-- Ende Overlay Play Button -->

        <widget
                id="mainBar"
                percentWidth="100"
                bottom="0"
                layoutMode="vertical"
                >




            <!-- Caption Layer -->

            <widget
                    id="caption"
                    type="caption"
                    face="caption"
                    percentWidth="100"
                    height="100"
                    verticalAlign="bottom"
                    layoutMode="vertical"
                    >

                <widget
                        id="captionLabel"
                        type="label"
                        top="10"
                        left="10"
                        right="10"
                        bottom="10"
                        font="captionFont"
                        />

            </widget>

            <!-- Ende Caption Layer -->

            <!-- CuePoint Layer -->

            <widget
                    id="cuepoint"
                    type="cuepoint"
                    layoutMode="none"
                    markerUpFace="cuepointUp"
                    markerDownFace="cuepointDown"
                    markerDisabledFace="cuepointDisabled"
                    font="defaultFont"
                    scrollSpeed="10"
                    scrollSensitivity="50"
                    percentWidth="100"
                    autoHide="true"
                    fadeSteps="7"
                    face="empty"
                    />

            <!-- Ende CuePoint Layer -->

            <!-- "Einzelsendung" Layer -->

            <widget
                    id="articleList"
                    type="articleList"
                    fadeSteps="5"
                    layoutMode="none"
                    markerUpFace="cuepointUp"
                    markerDownFace="cuepointDown"
                    markerDisabledFace="cuepointDisabled"
                    markerSelectedUpFace="cuepointSelectedUp"
                    markerSelectedDownFace="cuepointSelectedDown"
                    markerSelectedDisabledFace="cuepointSelectedDisabled"
                    font="defaultFont"
                    scrollSpeed="10"
                    scrollSensitivity="50"
                    left="0"
                    right="0"
                    face="empty"
                    autoHide="false"
                    />

            <!-- Ende "Einzelsendung" Layer -->


            <!-- Controlbar -->

            <widget
                    id="controlBar"
                    face="bar"
                    percentWidth="100"
                    height="44"
                    horizontalAlign="center"
                    bottom="0"
                    autoHide="true"
                    fadeSteps="7"
                    >

                <!-- Linker Controlbar Bereich -->

                <widget
                        id="controlBarLeft"
                        layoutMode="horizontal"
                        left="5"
                        percentHeight="100"
                        >

                    <widget
                            id="playPauseButton"
                            accessabilityName="Abspielen oder Pausieren"
                            type="playPauseButton"
                            verticalAlign="middle"
                            upFace="playUp"
                            downFace="playDown"
                            disabledFace="playDisabled"
                            selectedUpFace="pauseUp"
                            selectedDownFace="pauseDown"
                            selectedDisabledFace="pauseDisabled"
                            buttonMode="true"
                            tooltip="Play/Pause"
                            />

                    <widget
                            id="stopButton"
                            accessabilityName="Stoppen"
                            type="stopButton"
                            verticalAlign="middle"
                            upFace="stopUp"
                            downFace="stopDown"
                            disabledFace="stopDisabled"
                            tooltip="Stop"
                            />

                </widget>

                <!-- Ende Linker Controlbar Bereich -->

                <!-- Rechter Controlbar Bereich -->

                <widget
                        id="controlBarRight"
                        layoutMode="horizontal"
                        verticalAlign="middle"
                        percentHeight="100"
                        right="5"
                        >

                    <widget
                            id="timeField"
                            type="timeField"
                            displayMode="elapsedDuration"
                            verticalAlign="middle"
                            />

                    <widget width="8" />

                    <widget
                            id="volumeButton"
                            accessabilityName="Laustärkeregelung"
                            type="volumeButton"
                            verticalAlign="middle"
                            upFace="muteUp"
                            downFace="muteDown"
                            disabledFace="muteDisabled"
                            selectedUpFace="muteSelectedUp"
                            selectedDownFace="muteSelectedDown"
                            selectedDisabledFace="muteSelectedDisabled"
                            tooltip="Laustärkeregelung"
                            sliderHeight="55"
                            sliderWidth="24"
                            sliderBackgroundColor="0x111111"
                            sliderBackgroundAlpha="0.6"
                            thumbUp="volumeSliderThumbUp"
                            thumbDown="volumeSliderThumbDown"
                            thumbDisabled="volumeSliderThumbDisabled"
                            track="volumeSliderTrack"
                            progressTrack="volumeSliderProgressTrack"
                            value="0.75"
                            />

                    <widget width="2" />

                    <widget
                            id="adjustmentButton"
                            accessabilityName="Videoeinstellungen ändern"
                            type="adjustmentButton"
                            verticalAlign="middle"
                            upFace="adjustmentUp"
                            downFace="adjustmentDown"
                            disabledFace="adjustmentDisabled"
                            selectedUpFace="adjustmentSelectedUp"
                            selectedDownFace="adjustmentSelectedDown"
                            selectedDisabledFace="adjustmentSelectedDisabled"
                            buttonMode="true"
                            tooltip="Einstellungen"
                            />

                    <widget width="2" />

                    <widget
                            id="fullScreenButton"
                            accessabilityName="Vollbild ein oder ausschalten"
                            type="fullScreenButton"
                            verticalAlign="middle"
                            upFace="fullScreenEnterUp"
                            downFace="fullScreenEnterDown"
                            disabledFace="fullScreenEnterDisabled"
                            selectedUpFace="fullScreenLeaveUp"
                            selectedDownFace="fullScreenLeaveDown"
                            selectedDisabledFace="fullScreenLeaveDisabled"
                            buttonMode="true"
                            tooltip="Vollbild"
                            />

                    <widget width="2" />

                    <widget
                            id="captionButton"
                            type="captionButton"
                            verticalAlign="middle"
                            upFace="captionUp"
                            downFace="captionDown"
                            disabledFace="captionDisabled"
                            selectedUpFace="captionSelectedUp"
                            selectedDownFace="captionSelectedDown"
                            selectedDisabledFace="captionSelectedDisabled"
                            buttonMode="true"
                            tooltip="Untertitel"
                            />

                    <widget width="2" />

                    <widget
                            id="infoButton"
                            accessabilityName="Informationen zum Video anzeigen"
                            type="infoButton"
                            verticalAlign="middle"
                            upFace="infoUp"
                            downFace="infoDown"
                            disabledFace="infoDisabled"
                            selectedUpFace="infoSelectedUp"
                            selectedDownFace="infoSelectedDown"
                            selectedDisabledFace="infoSelectedDisabled"
                            buttonMode="true"
                            tooltip="Informationen zum Beitrag"
                            />

                </widget>

                <!-- Ende Rechter Controlbar Bereich -->

                <!-- Scrubbar -->

                <widget
                        id="scrubBar"
                        type="scrubBar"
                        showTimeFields="false"
                        left="90"
                        right="205"
                        height="17"
                        scrubberUp="scrubberUp"
                        scrubberDown="scrubberDown"
                        scrubberDisabled="scrubberDisabled"
                        track="scrubBarTrack"
                        progressTrack="scrubBarProgressTrack"
                        showDataTip="true"
                        immediateScrubbing="false"
                        dataTipMode="elapsedDuration"
                        verticalAlign="middle"
                        />

                <!-- Ende Scrubbar -->

            </widget>


        </widget>

        <!-- Ende Controlbar -->

		<!-- Loader Animation -->
		
		<widget
			id="loaderAnimation"
			type="loaderAnimation"
			horizontalAlign="center"
			verticalAlign="middle"
			/>
					
		<!-- Ende Loader Animation -->
		
		<!-- Alert Dialog -->

		<widget
			id="alert"
			type="alert"
			face="panel"
			width="250"
			height="140"
			verticalAlign="middle"
			horizontalAlign="center"
			fadeSteps="6"
			dropShadow="true"
			>
			
			<widget
				id="captionLabel"
				type="label"
				top="12"
				left="15"
				right="10"
				height="18"
				fontSize="14"
				/>
				
			<widget
				id="messageLabel"
				type="label"
				top="44"
				left="15"
				right="15"
				bottom="10"
				multiline="true"
				selectable="true"
				/>
				
			<widget
				id="closeButton"
				type="button"
				upFace="closeUp"
				downFace="closeDown"
				disabledFace="closeDisabled"
				right="10"
				top="10"
				tooltip="Meldung schliessen"
				/>
			
		</widget>

		<!-- Ende Alert Dialog -->
				
	</widgets>
	
